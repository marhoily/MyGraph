<UserControl x:Class="MyGraph.MapView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:myGraph="clr-namespace:MyGraph"
             xmlns:windowsPresentation="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             d:DataContext="{d:DesignInstance myGraph:MapViewModel}">
    <Grid>
        <windowsPresentation:GMapControl x:Name="_gMapControl"/>
        <myGraph:GraphView x:Name="_graphView"/>
    </Grid>
</UserControl>